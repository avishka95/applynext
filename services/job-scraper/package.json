{
    "name": "job-scraper",
    "version": "0.1.0",
    "description": "Job scraper service",
    "main": "index.js",
    "scripts": {
        "build": "tsc --build",
        "start": "node dist/src/service.js",
        "dev": "ts-node-dev --watch --respawn --transpile-only src/service.ts",
        "test": "npx jest",
        "test:watch": "npx jest --watch",
        "test:coverage": "npx jest --coverage",
        "test:unit": "npx jest --selectProjects unit",
        "test:unit:watch": "npx jest --watch --selectProjects unit",
        "test:unit:coverage": "npx jest --coverage --selectProjects unit",
        "test:integration": "npx jest --selectProjects integration",
        "test:integration:watch": "npx jest --watch --selectProjects integration",
        "test:integration:coverage": "npx jest --coverage --selectProjects integration",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prettier:check": "prettier --check .",
        "prettier:write": "prettier --write .",
        "prettier:fix": "prettier --write ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@google/genai": "^1.13.0",
        "@types/express": "^5.0.3",
        "@types/node": "^24.2.1",
        "dotenv": "^17.2.1",
        "express": "^5.1.0",
        "pg": "^8.16.3",
        "playwright": "^1.54.2",
        "playwright-core": "^1.54.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.2"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/pg": "^8.15.5",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.40.0",
        "@typescript-eslint/parser": "^8.40.0",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "jest": "^30.0.5",
        "prettier": "^3.6.2",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.1"
    }
}
